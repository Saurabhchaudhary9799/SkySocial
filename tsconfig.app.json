{
  "compilerOptions": {
    "jsx": "react-jsx", // Use "react" for older versions of React (before React 17)
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "lib": ["DOM", "ESNext"],
    "strict": true,
    "skipLibCheck": true,
    
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
    
  }
}
